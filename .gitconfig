[user]
	email = punteney@gmail.com
	name = James Punteney
[alias]
	ci = commit
	st = status
	br = branch
	co = checkout
	df = diff
	lg = log -p
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	excludesfile = /Users/james/.gitignore
